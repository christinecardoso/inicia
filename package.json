    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.14",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.3.1",
    "cross-env": "^7.0.3",
    "browserslist": "^4.16.6"
    "vite-plugin-pwa": "^0.14.1"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.0",
