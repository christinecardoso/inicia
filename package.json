{
  "name": "inicia",
  "version": "1.0.0",
  "description": "Based on 11ty Rapid Starter. 11ty-alpine-vite-tailwind",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli _site",
    "prepare": "npx husky install",
    "preview": "npx vite preview",
    "predev": "run-s clean predev:*",
    "predev:submodules": "git submodule update --init --recursive",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:submodules": "git submodule update --recursive --remote",
    "dev:eleventy": "npx @11ty/eleventy --serve --incremental",
    "serve": "npx @11ty/eleventy --serve",
    "watch": "npx @11ty/eleventy --watch",
    "prebuild": "run-s clean prebuild:submodules",
    "prebuild:submodules": "git submodule update --init --recursive",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:eleventy": "npx @11ty/eleventy",
    "build:submodules": "git submodule update --recursive --remote",
    "setup": "npm install && npm run validate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.8",
    "@babel/eslint-parser": "^7.22.7",
    "@babel/preset-env": "^7.22.7",
    "@cc/sapphire": "github:christinecardoso/11ty-sapphire",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.16.6",
    "cross-env": "^7.0.3",
    "daisyui": "^4.4.21",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.3",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-eleventy-img": "^0.10.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.0.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.5",
    "vite-plugin-pwa": "^0.14.1"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.0",
    "@alpinejs/persist": "^3.11.1",
    "@sindresorhus/slugify": "^2.2.0",
    "alpinejs": "^3.12.0",
    "core-js": "^3.20.2",
    "lazysizes": "^5.3.2",
    "nodemailer": "^6.9.3",
    "slugify": "^1.6.6",
    "swiper": "^11.0.5"
  }
}
