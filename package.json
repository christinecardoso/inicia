{
  "name": "Inicia",
  "version": "1.0.0",
  "description": "Based on 11ty Rapid Starter. 11ty-alpine-vite-tailwind",
  "main": "index.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "dev": "NODE_ENV=development npx @11ty/eleventy --serve --incremental",
    "serve": "npx @11ty/eleventy --serve",
    "watch": "npx @11ty/eleventy --watch",
    "prebuild": "run-s clean",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:eleventy": "npx @11ty/eleventy",
    "clean": "del-cli _site",
    "setup": "npm install && npm run validate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
